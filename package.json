{
  "name": "autorender-bug",
  "version": "0.0.0",
  "main": "autorender-bug/server.stache!done-autorender",
  "description": "",
  "homepage": "",
  "scripts": {
    "test": "testee src/test.html --browsers firefox --reporter Spec",
    "startds": "done-serve --port 8080",
    "start": "NODE_ENV=production node ./bin/www",
    "develop": "done-serve --develop --port 8080",
    "document": "documentjs",
    "build": "node build",
    "build:server": "steal-tools build --no-minify"
  },
  "author": {
    "name": "",
    "email": "",
    "url": ""
  },
  "keywords": [],
  "system": {
    "ext": {
      "ejs": "can/view/ejs/system",
      "mustache": "can/view/mustache/system",
      "stache": "can/view/stache/system"
    },
    "plugins": [
      "done-css",
      "steal-less",
      "can"
    ],
    "directories": {
      "lib": "src"
    },
    "configDependencies": [
      "live-reload",
      "node_modules/can-zone/register"
    ],
    "transpiler": "babel",
    "npmAlgorithm": "flat"
  },
  "dependencies": {
    "can": "^2.3.33",
    "can-connect": "^0.5.0",
    "can-zone": "^0.5.0",
    "done-autorender": "^0.8.0",
    "done-component": "^0.4.0",
    "done-css": "~2.0.2",
    "done-serve": "^0.2.0",
    "done-ssr": "^0.13.5",
    "generator-donejs": "^0.9.0",
    "jquery": "~2.2.1",
    "steal": "^1.6.2"
  },
  "devDependencies": {
    "documentjs": "^0.4.2",
    "donejs-deploy": "^0.4.0",
    "funcunit": "~3.0.0",
    "steal-qunit": "^0.1.1",
    "steal-less": "^1.2.2",
    "steal-tools": "^1.10.0",
    "testee": "^0.2.4",
    "donejs-cli": "^0.9.4",
    "can-fixture": "^0.1.2"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "useBuiltIns": "usage",
          "targets": {
            "node": "8"
          }
        }
      ]
    ]
  }
}
